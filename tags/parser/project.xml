<?xml version="1.0"?>
<project>
  <extend>../project.xml</extend>
  <pomVersion>3</pomVersion>

  <id>parser</id>

  <name>Parser</name>

  <groupId>czt</groupId>

  <currentVersion>0.1</currentVersion>

  <package>net.sourceforge.czt.parser</package>

  <shortDescription>A Z parser.</shortDescription>

  <repository>
    <connection>
      scm:cvs:pserver:anonymous@cvs.sourceforge.net:/cvsroot/czt:parser
