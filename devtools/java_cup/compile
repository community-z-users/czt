#CUP=/usr/java/java_cup/
CUP=../../../lib/java_cup.jar
cd src/
java -cp $CUP net.sourceforge.czt.java_cup.Main < net/sourceforge/czt/java_cup/parser.cup
#java -cp $CUP java_cup.Main < net/sourceforge/czt/java_cup/parser.cup
sed 's/java_cup/net.sourceforge.czt.java_cup/g' < parser.java > newparser.java
sed 's/net.sourceforge.czt.net/net/g' < newparser.java > net/sourceforge/czt/java_cup/parser.java
mv sym.java net/sourceforge/czt/java_cup/
