\begin{zsection}
   \SECTION time\_process\_endby \parents circustime\_toolkit
\end{zsection}


\begin{axdef}
   tval: \nat
\where
   tval > 20 \land tval<30
\end{axdef}



\begin{circus}
   \circchannel\ c, d \\
\end{circus}


%Process P with circus action A 

\begin{circus}
    \circprocess\ P \circdef  \circbegin  \\
    \t1 A \circdef c1 \then \Skip \\
    \t1 \circspot A \\
    \circend
\end{circus}


%Process Q with circus action B

\begin{circus}
    \circprocess\ Q \circdef  \circbegin  \\
    \t1 B \circdef c2 \then \Skip \\
    \t1 \circspot B \\
    \circend
\end{circus}


%Interface parallelism with explicit name sets.

\begin{circus}
    \circprocess Pro16 \circdef \lcirctime 20 \rcirctime \circstartby Q \lpar \{~ tval ~\} | \lchanset c, d \rchanset | \{~ tval ~\} \rpar  \lcirctime 20 \rcirctime \circstartby Q \\
\end{circus}

\begin{circus}
    \circprocess Pro17 \circdef ( \lcirctime 20 \rcirctime \circstartby Q) \lpar \{~ tval ~\} | \lchanset c, d \rchanset | \{~ tval ~\} \rpar ( \lcirctime 25 \rcirctime \circstartby Q) \\
\end{circus}


%Alphabetised parallelism with default (empty) name sets for both sides. Notice the required extra BAR (\verb'|') after/before the parallel brackets.

\begin{circus}
    \circprocess Pro18 \circdef  \lcirctime 20 \rcirctime \circstartby Q [ | \lchanset c \rchanset || \lchanset c, d \rchanset | ]  \lcirctime 20 \rcirctime \circstartby Q \\
\end{circus}
 
\begin{circus}
    \circprocess Pro19 \circdef ( \lcirctime 20 \rcirctime \circstartby Q) [ | \lchanset c \rchanset || \lchanset c, d \rchanset | ] ( \lcirctime 25 \rcirctime \circstartby Q) \\
\end{circus}



\begin{circus}
    \circprocess Pro20 \circdef \circvres tvar : \nat  \circspot  \lcirctime tvar \rcirctime \circstartby Q\\
\end{circus}   


\begin{circus}
    \circprocess Pro12 \circdef (c \then  \lcirctime 10 \rcirctime \circstartby Q) \circhide c \\
\end{circus} 
     
\begin{circus}
    \t1 Pro26 \circdef \lcirctime X : 0 \upto 10 \rcirctime \circstartby Q\\
\end{circus}        

\begin{circus}
    \t1 Pro27 \circdef \lcirctime X : 0 \upto tval \rcirctime \circstartby Q \\
\end{circus} 
