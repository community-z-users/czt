\begin{zsection}
   \SECTION time\_process\_startby \parents circustime\_toolkit
\end{zsection}


\begin{axdef}
   tval: \nat
\where
   tval > 20 \land tval<30
\end{axdef}



\begin{circus}
   \circchannel\ c, d \\
\end{circus}


%Process P with circus action A 

\begin{circus}
    \circprocess\ P \circdef  \circbegin  \\
    \t1 A \circdef c1 \then \Skip \\
    \t1 \circspot A \\
    \circend
\end{circus}


%Process Q with circus action B

\begin{circus}
    \circprocess\ Q \circdef  \circbegin  \\
    \t1 B \circdef c2 \then \Skip \\
    \t1 \circspot B \\
    \circend
\end{circus}


\begin{circus}
    \circprocess Pro1 \circdef  \lcirctime 20 \rcirctime \circstartby Q \\
\end{circus}

\begin{circus}
    \circprocess Pro2 \circdef P \circendby \lcirctime 20 \rcirctime  \circseq P \circendby \lcirctime 20 \rcirctime  \\
\end{circus}
    
\begin{circus}
    \circprocess Pro3 \circdef (P \circendby \lcirctime 20 \rcirctime ) \circseq (P \circendby \lcirctime 25 \rcirctime ) \\
\end{circus}
    
\begin{circus}
    \circprocess Pro4 \circdef P \circendby \lcirctime 20 \rcirctime  \extchoice P \circendby \lcirctime 20 \rcirctime  \\ 
\end{circus}
    
\begin{circus}
    \circprocess Pro5 \circdef (P \circendby \lcirctime 20 \rcirctime ) \extchoice (P \circendby \lcirctime 25 \rcirctime ) \\
\end{circus}


\begin{circus}
    \circprocess Pro6 \circdef P \circendby \lcirctime 20 \rcirctime  \intchoice P \circendby \lcirctime 20 \rcirctime  \\
\end{circus}
    
\begin{circus}
    \circprocess Pro7 \circdef (P \circendby \lcirctime 20 \rcirctime ) \intchoice (P \circendby \lcirctime 25 \rcirctime ) \\
\end{circus}
    
\begin{circus}
    \circprocess Pro8 \circdef P \circendby \lcirctime 20 \rcirctime  \interleave P \circendby \lcirctime 20 \rcirctime  \\
\end{circus}

\begin{circus}
    \circprocess Pro9 \circdef (P \circendby \lcirctime 20 \rcirctime ) \interleave (P \circendby \lcirctime 25 \rcirctime ) \\
\end{circus}


%Interface parallelism

\begin{circus}
    \circprocess Pro10 \circdef P \circendby \lcirctime 20 \rcirctime  \lpar  \lchanset c, d \rchanset  \rpar  P \circendby \lcirctime 20 \rcirctime  \\
\end{circus}

\begin{circus}
    \circprocess Pro11 \circdef (P \circendby \lcirctime 20 \rcirctime ) \lpar  \lchanset c, d \rchanset  \rpar (P \circendby \lcirctime 25 \rcirctime ) \\
\end{circus}


\begin{circus}
    \circprocess Pro12 \circdef (P \circendby \lcirctime 10 \rcirctime ) \circhide c \\
\end{circus} 


\begin{circus}
    \circprocess Pro13 \circdef P \circendby \lcirctime tval \rcirctime \\
\end{circus}        

\begin{circus}
   \circprocess Pro14 \circdef P \circendby \lcirctime tval*2+5 \rcirctime \\
\end{circus}        

\begin{circus}
    \circprocess Pro15 \circdef P \circendby \lcirctime 0 \upto 10 \rcirctime \\
\end{circus}        

\begin{circus}
    \circprocess Pro16 \circdef P \circendby \lcirctime 0 \upto tval \rcirctime  \\
\end{circus} 







