\begin{zsection}
   \SECTION time\_action\_endby \parents circustime\_toolkit
\end{zsection}


\begin{axdef}
   tval: \nat
\where
   tval > 20 \land tval<30
\end{axdef}


\begin{circus}
   \circchannel\ c, d \\
\end{circus}

\begin{circus}
    \circprocess\ P \circdef \circbegin \\
\end{circus}


\begin{circusaction}
    	\t1 A \circdef c \then \Skip \\
\end{circusaction}

\begin{circusaction}
        \t1 B \circdef d \then \Skip \\
\end{circusaction}
     
\begin{circusaction}
        \t1 Act1 \circdef  A \circendby \lcirctime 20 \rcirctime  \\
\end{circusaction}

\begin{circusaction}
        \t1 Act2 \circdef  A \circendby \lcirctime 20 \rcirctime \circseq A \circendby \lcirctime 20 \rcirctime \\
\end{circusaction}

\begin{circusaction}
        \t1 Act3 \circdef ( A \circendby \lcirctime 20 \rcirctime )  \circseq ( A \circendby \lcirctime 20 \rcirctime )\\
\end{circusaction}

\begin{circusaction}
        \t1 Act4 \circdef  A \circendby \lcirctime 20 \rcirctime \extchoice A \circendby \lcirctime 20 \rcirctime \\
\end{circusaction}

\begin{circusaction}
        \t1 Act5 \circdef ( A \circendby \lcirctime 20 \rcirctime )  \extchoice ( A \circendby \lcirctime 20 \rcirctime  )\\
\end{circusaction}

\begin{circusaction}
        \t1 Act6 \circdef  A \circendby \lcirctime 20 \rcirctime   \intchoice A \circendby \lcirctime 20 \rcirctime\\
\end{circusaction}

\begin{circusaction}
        \t1 Act7 \circdef ( A \circendby \lcirctime 20 \rcirctime )  \intchoice ( A \circendby \lcirctime 20 \rcirctime )\\
\end{circusaction}

\begin{circusaction}
        \t1 Act8 \circdef  A \circendby \lcirctime 20 \rcirctime \interleave A \circendby \lcirctime 20 \rcirctime \\
\end{circusaction}

\begin{circusaction}
        \t1 Act9 \circdef ( A \circendby \lcirctime 20 \rcirctime )  \interleave ( A \circendby \lcirctime 20 \rcirctime )\\
\end{circusaction}

\begin{circusaction}
        \t1 Act10 \circdef A \circendby \lcirctime 20 \rcirctime  \circinterrupt A \circendby \lcirctime 20 \rcirctime \\
\end{circusaction}

\begin{circusaction}
        \t1 Act11 \circdef ( A \circendby \lcirctime 20 \rcirctime )  \circinterrupt ( A \circendby \lcirctime 20 \rcirctime  )\\
\end{circusaction}

%Interface parallelism with default (empty) name sets for both sides. Notice the required extra BAR (\verb'|') after/before the parallel brackets.

\begin{circusaction}
        \t1 Act12 \circdef A \circendby \lcirctime 20 \rcirctime  \lpar | \lchanset c, d \rchanset | \rpar A \circendby \lcirctime 20 \rcirctime\\
\end{circusaction}

\begin{circusaction}
        \t1 Act13 \circdef ( A \circendby \lcirctime 20 \rcirctime )  \lpar | \lchanset c, d \rchanset | \rpar ( A \circendby \lcirctime 20 \rcirctime)\\
\end{circusaction}

%Interface parallelism with explicit name sets.

\begin{circusaction}
        \t1 Act14 \circdef  A \circendby \lcirctime 20 \rcirctime   \lpar \{~ tval ~\} | \lchanset c, d \rchanset | \{~ tval ~\} \rpar A \circendby \lcirctime 20 \rcirctime \\
\end{circusaction}

\begin{circusaction}
        \t1 Act15 \circdef ( A \circendby \lcirctime 20 \rcirctime )  \lpar \{~ tval ~\} | \lchanset c, d \rchanset | \{~ tval ~\} \rpar ( A \circendby \lcirctime 20 \rcirctime )\\
\end{circusaction}

%Alphabetised parallelism with default (empty) name sets for both sides. Notice the required extra BAR (\verb'|') after/before the parallel brackets.

\begin{circusaction}
        \t1 Act16 \circdef A \circendby \lcirctime 20 \rcirctime  [ | \lchanset c \rchanset || \lchanset c, d \rchanset | ] A \circendby \lcirctime 20 \rcirctime\\
\end{circusaction}

\begin{circusaction}
        \t1 Act17 \circdef ( A \circendby \lcirctime 20 \rcirctime)  [ | \lchanset c \rchanset || \lchanset c, d \rchanset | ] ( A \circendby \lcirctime 20 \rcirctime )\\
\end{circusaction}

%recursive

\begin{circusaction}
	\t1 Act18 \circdef \circmu~X \circspot A \circendby \lcirctime 20 \rcirctime  \circseq X \\
\end{circusaction}


\begin{circusaction}
	\t1 Act19 \circdef \circvres vart : \nat  \circspot  A \circendby \lcirctime vart \rcirctime  \\
\end{circusaction}


\begin{circusaction}
	\t1 Act20 \circdef c \then  A \circendby \lcirctime 10 \rcirctime \\
\end{circusaction}

\begin{circusaction}
	\t1 Act21 \circdef c \then  \Skip \circendby \lcirctime 10 \rcirctime \\
\end{circusaction}


\begin{circusaction}
	\t1 Act22 \circdef c \then    \lcirctime 10 \rcirctime \circstartby \Stop \\
\end{circusaction}


\begin{circusaction}
	\t1 Act23 \circdef   \lcirctime tval \rcirctime \circstartby B \\
\end{circusaction}

\begin{circusaction}
	\t1 Act24 \circdef   \lcirctime tval*2+5 \rcirctime \circstartby B \\
\end{circusaction}
 


\begin{circusaction}
    	\t1 Act25 \circdef  \lcirctime 0 \upto 10 \rcirctime \circstartby B \\
\end{circusaction}

\begin{circusaction}
    	\t1 Act26 \circdef   \lcirctime 0 \upto tval \rcirctime \circstartby B \\
\end{circusaction}



\begin{circusaction}        
        \t1 \circspot A \circseq B\\
\end{circusaction}

\begin{circus}    
\circend
\end{circus}
