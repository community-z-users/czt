\begin{zsection}
   \SECTION timeout\_action \parents circustime\_toolkit
\end{zsection}


\begin{axdef}
   tval: \nat
\where
   tval > 20 \land tval<30
\end{axdef}



\begin{circus}
   \circchannel\ c, d \\
\end{circus}

\begin{circus}
    \circprocess\ P \circdef \circbegin \\
\end{circus}

\begin{circusaction}
    	\t1 A \circdef c \then \Skip \\
\end{circusaction}

\begin{circusaction}
        \t1 B \circdef d \then \Skip \\
\end{circusaction}
                
\begin{circusaction}
        \t1 Act1 \circdef A \circtimeout \lcirctime 20 \rcirctime B \\
\end{circusaction}

\begin{circusaction}         
	\t1 Act2 \circdef A \circtimeout \lcirctime 20 \rcirctime B \circseq A \circtimeout \lcirctime 20 \rcirctime B \\
\end{circusaction}

\begin{circusaction} 
        \t1 Act3 \circdef (A \circtimeout \lcirctime 20 \rcirctime B) \circseq (A \circtimeout \lcirctime 25 \rcirctime B) \\
\end{circusaction}

\begin{circusaction}
	\t1 Act4 \circdef A \circtimeout \lcirctime 20 \rcirctime B \extchoice A \circtimeout \lcirctime 20 \rcirctime B \\
\end{circusaction}

\begin{circusaction}
        \t1 Act5 \circdef (A \circtimeout \lcirctime 20 \rcirctime B) \extchoice (A \circtimeout \lcirctime 25 \rcirctime B) \\
\end{circusaction}

\begin{circusaction}
	\t1 Act6 \circdef A \circtimeout \lcirctime 20 \rcirctime B \intchoice A \circtimeout \lcirctime 20 \rcirctime B \\
\end{circusaction}

\begin{circusaction}
        \t1 Act7 \circdef (A \circtimeout \lcirctime 20 \rcirctime B) \intchoice (A \circtimeout \lcirctime 25 \rcirctime B) \\
\end{circusaction}

\begin{circusaction}
	\t1 Act8 \circdef A \circtimeout \lcirctime 20 \rcirctime B \interleave A \circtimeout \lcirctime 20 \rcirctime B \\
\end{circusaction}

\begin{circusaction}
        \t1 Act9 \circdef (A \circtimeout \lcirctime 20 \rcirctime B) \interleave (A \circtimeout \lcirctime 25 \rcirctime B) \\
\end{circusaction}

\begin{circusaction}
        \t1 Act10 \circdef A \circtimeout \lcirctime 20 \rcirctime B \circinterrupt  A \circtimeout \lcirctime 20 \rcirctime B \\
\end{circusaction}

\begin{circusaction}
        \t1 Act11 \circdef (A \circtimeout \lcirctime 20 \rcirctime B) \circinterrupt (A \circtimeout \lcirctime 25 \rcirctime B) \\
\end{circusaction}


%Interface parallelism with default (empty) name sets for both sides. Notice the required extra BAR (\verb'|') after/before the parallel brackets.
\begin{circusaction}
        \t1 Act12 \circdef A \circtimeout \lcirctime 20 \rcirctime B \lpar | \lchanset c, d \rchanset | \rpar  A \circtimeout \lcirctime 20 \rcirctime B \\
\end{circusaction}

\begin{circusaction}
        \t1 Act13 \circdef (A \circtimeout \lcirctime 20 \rcirctime B) \lpar | \lchanset c, d \rchanset | \rpar (A \circtimeout \lcirctime 25 \rcirctime B) \\
\end{circusaction}
        
%Interface parallelism with explicit name sets.
\begin{circusaction}
        \t1 Act14 \circdef A \circtimeout \lcirctime 20 \rcirctime B \lpar \{~ tval ~\} | \lchanset c, d \rchanset | \{~ tval ~\} \rpar A \circtimeout \lcirctime 20 \rcirctime B \\
\end{circusaction}

\begin{circusaction}
        \t1 Act15 \circdef (A \circtimeout \lcirctime 20 \rcirctime B) \lpar \{~ tval ~\} | \lchanset c, d \rchanset | \{~ tval ~\} \rpar (A \circtimeout \lcirctime 25 \rcirctime B) \\
\end{circusaction}
   
%Alphabetised parallelism with default (empty) name sets for both sides. Notice the required extra BAR (\verb'|') after/before the parallel brackets.
\begin{circusaction}
        \t1 Act16 \circdef A \circtimeout \lcirctime 20 \rcirctime B [ | \lchanset c \rchanset || \lchanset c, d \rchanset | ] A \circtimeout \lcirctime 20 \rcirctime B \\
\end{circusaction}

\begin{circusaction}
        \t1 Act17 \circdef (A \circtimeout \lcirctime 20 \rcirctime B) [ | \lchanset c \rchanset || \lchanset c, d \rchanset | ] (A \circtimeout \lcirctime 25 \rcirctime B) \\
\end{circusaction}

\begin{circusaction}
	\t1 Act18 \circdef \circmu~X \circspot A \circtimeout \lcirctime 20 \rcirctime B \circseq X \\
\end{circusaction}

\begin{circusaction}
	\t1 Act19 \circdef \circvres tvar : \num \circspot A \circtimeout \lcirctime tvar \rcirctime B \\
\end{circusaction}

\begin{circusaction}
	\t1 Act20 \circdef c \then  A \circtimeout \lcirctime 10 \rcirctime B \\
\end{circusaction}

\begin{circusaction}
	\t1 Act21 \circdef c \then  A \circtimeout \lcirctime 10 \rcirctime \Skip \\
\end{circusaction}

\begin{circusaction}
	\t1 Act22 \circdef c \then  A \circtimeout \lcirctime 10 \rcirctime \Stop \\
\end{circusaction}

\begin{circusaction}
	\t1 Act23 \circdef (c \then A \circtimeout \lcirctime 10 \rcirctime B) \circhide c \\
\end{circusaction}

\begin{circusaction}
	\t1 Act24 \circdef A \circtimeout \lcirctime tval \rcirctime B \\
\end{circusaction}

\begin{circusaction}
	\t1 Act25 \circdef A \circtimeout \lcirctime tval*2+5 \rcirctime B \\
\end{circusaction}
 

\begin{circusaction}
    	\t1 Act26 \circdef A \circtimeout \lcirctime 0 \upto 10 \rcirctime B \\
\end{circusaction}

\begin{circusaction}
    	\t1 Act27 \circdef A \circtimeout \lcirctime 0 \upto tval \rcirctime B \\
\end{circusaction}




\begin{circusaction}
        \t1 \circspot A \circseq B\\
\end{circusaction}

\begin{circus}
    \circend
\end{circus}

