# Plugin properties

# Basic plugin information properties
plugin.zsidekick.ZSideKickPlugin.activate=defer
plugin.zsidekick.ZSideKickPlugin.name=ZSideKick
plugin.zsidekick.ZSideKickPlugin.author=Petra Malik
plugin.zsidekick.ZSideKickPlugin.version=${project.version}
plugin.zsidekick.ZSideKickPlugin.jars=czt.jar
plugin.zsidekick.ZSideKickPlugin.docs=zsidekick.html

# Plugin dependency properties
plugin.zsidekick.ZSideKickPlugin.depend.0=jdk 1.6
plugin.zsidekick.ZSideKickPlugin.depend.1=jedit 04.00.04.00
plugin.zsidekick.ZSideKickPlugin.depend.2=plugin errorlist.ErrorListPlugin 1.3
plugin.zsidekick.ZSideKickPlugin.depend.3=plugin sidekick.SideKickPlugin 0.3

# Plugin menu item properties
plugin.zsidekick.ZSideKickPlugin.menu=sidekick-tree \
                                      - \
                                      zsidekick.toLatex \
                                      zsidekick.toOldLatex \
                                      zsidekick.toUnicode \
                                      zsidekick.toXml \
                                      zsidekick.domainCheck \
                                      zsidekick.fsbCheck \
                                      zsidekick.nextWff \
                                      zsidekick.insertTypeForCurrentWff \
                                      zsidekick.rewriteCurrentWff \
                                      zsidekick.interactiveRewriteCurrentWff \
                                      zsidekick.proveCurrentWff \
                                      - \
                                      zsidekick.disableTypechecking \
                                      zsidekick.show_current_sm_options \
                                      zsidekick.reset_sm
#                                      zsidekick.show_complete_tree
#                                      zsidekick.gotoDefinition 

# action labels
sidekick-tree.label=Structure Browser
zsidekick.toLatex.label=Convert to Latex mark-up
zsidekick.toOldLatex.label=Convert to old Latex mark-up
zsidekick.toUnicode.label=Convert to Unicode mark-up
zsidekick.toXml.label=Convert to XML
zsidekick.domainCheck.label=Domain check buffer
zsidekick.fsbCheck.label=Feasibility check buffer
zsidekick.nextWff.label=Highlight next wff
#zsidekick.gotoDefinition.label=Goto definition of selected referencing name
zsidekick.insertTypeForCurrentWff.label=Insert type of selected formula
zsidekick.proveCurrentWff.label=Prove selected predicate
zsidekick.rewriteCurrentWff.label=Rewrite selected formula
zsidekick.interactiveRewriteCurrentWff.label=Show proof tree for rewriting selected formula
zsidekick.disableTypechecking.label=Disable Typechecking
#zsidekick.showCompleteTree.label=Show complete syntax tree
zsidekick.show_current_sm_options.label=Show section manager options
zsidekick.reset_sm.label=Reset section manager

# Option defaults
zsidekick.ignore_unknown_latex_commands=false
zsidekick.print_name_ids=false
zsidekick.print_z_eves=false
zsidekick.txt_witdh=80
zsidekick.typecheck_use_before_decl=false
zsidekick.typecheck_raise_warnings=true
zsidekick.typecheck_recursive_types=false
zsidekick.typecheck_use_strong_typing=false
zsidekick.vcg_process_parents=false
zsidekick.vcg_add_trivial_vc=false
zsidekick.vcg_apply_transformers=true
#zsidekick.vcg_raise_type_warnings=true    - use just one raise-type-warning
zsidekick.vcg_dc_use_infix_appliesto=true
zsidekick.vcg_fsb_add_givenset_vcs=true
zsidekick.vcg_fsb_create_zschemas=false
zsidekick.debugZsideKick=false
#zsidekick.czt.path=./
zsidekick.show_complete_tree=false

# Option pane
plugin.zsidekick.ZSideKickPlugin.option-pane=zsidekick
options.zsidekick.label=ZSideKick
options.zsidekick.code=new zsidekick.OptionPane();
options.zsidekick.standardConformance=<html><h2>Extensions to Z Standard</h2></html>
# Parser / printer properties
options.zsidekick.show_complete_tree=Show complete syntax tree
options.zsidekick.ignore_unknown_latex_commands=Allow latex commands that do not have mark-up directives
options.zsidekick.print_name_ids=Print the Ids of names
options.zsidekick.print_z_eves=Print using (old) Spivey's Z
options.zsidekick.txt_witdh=Collumn to break line when printing
#options.zsidekick.addSpaceBeforeLatexPunctation=Add space before latex punctation (, and ; characters)
# Typechecker properties
options.zsidekick.typecheck_use_before_decl=Allow use of variables before their declaration
options.zsidekick.typecheck_raise_warnings=Raise warnings as errors
options.zsidekick.typecheck_recursive_types=Use recursive types
options.zsidekick.typecheck_use_strong_typing=Use strong typing for typechecking Object-Z specifications
# VCG properties
options.zsidekick.vcg_process_parents=Process parents during VCG
options.zsidekick.vcg_add_trivial_vc=Add trivial VCs to VCG results
options.zsidekick.vcg_apply_transformers=Apply term transformers during VCG
#options.zsidekick.vcg_raise_type_warnings=Raise VCG warnings as errors
options.zsidekick.vcg_dc_use_infix_appliesto=Use infix appliesTo for DC VCG
options.zsidekick.vcg_fsb_add_givenset_vcs=Create VC for given sets for FSB VCG
options.zsidekick.vcg_fsb_create_zschemas=Create extra schemas for PRE in FSB VCG
# jEdit related
options.zsidekick.resetButton=Reset all options to be Z Standard conforming
options.zsidekick.debugZsideKick=Switch to debug mode; it logs information on JEdit-SectionManager.log
options.zsidekick.cztPathLabel=CZT search paths
