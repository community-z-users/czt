<?xml version="1.0"?>

<!DOCTYPE ACTIONS SYSTEM "actions.dtd">

<ACTIONS>
  <ACTION NAME="zsidekick.toLatex">
    <CODE>
      zsidekick.ZSideKickActions.toLatex(view);
    </CODE>
  </ACTION>
  <ACTION NAME="zsidekick.toOldLatex">
    <CODE>
      zsidekick.ZSideKickActions.toOldLatex(view);
    </CODE>
  </ACTION>
  <ACTION NAME="zsidekick.toUnicode">
    <CODE>
      zsidekick.ZSideKickActions.toUnicode(view);
    </CODE>
  </ACTION>
  <ACTION NAME="zsidekick.toXml">
    <CODE>
      zsidekick.ZSideKickActions.toXml(view);
    </CODE>
  </ACTION>
  <ACTION NAME="zsidekick.domainCheck">
    <CODE>
      zsidekick.ZSideKickActions.vcg(view,
        net.sourceforge.czt.vcg.z.dc.DomainCheckUtils.getDCUtils(),
        net.sourceforge.czt.vcg.z.dc.DomainCheckPropertyKeys.VCG_DOMAINCHECK_SOURCENAME_SUFFIX);
    </CODE>
  </ACTION>
  <ACTION NAME="zsidekick.fsbCheck">
    <CODE>
      zsidekick.ZSideKickActions.vcg(view,
        net.sourceforge.czt.vcg.z.feasibility.FeasibilityUtils.getFeasibilityUtils(),
        net.sourceforge.czt.vcg.z.feasibility.FeasibilityPropertyKeys.VCG_FEASIBILITY_SOURCENAME_SUFFIX);
    </CODE>
  </ACTION>
  <ACTION NAME="zsidekick.nextWff">
    <CODE>
      zsidekick.ZSideKickActions.highlightNextWff(view);
    </CODE>
  </ACTION>
  <ACTION NAME="zsidekick.gotoDefinition">
    <CODE>
      zsidekick.ZSideKickActions.gotoDefinition(view);
    </CODE>
  </ACTION>
  <ACTION NAME="zsidekick.insertTypeForCurrentWff">
    <CODE>
      zsidekick.ZSideKickActions.insertTypeForCurrentWff(view);
    </CODE>
  </ACTION>
  <ACTION NAME="zsidekick.proveCurrentWff">
    <CODE>
      zsidekick.ZSideKickActions.prove(view);
    </CODE>
  </ACTION>
  <ACTION NAME="zsidekick.rewriteCurrentWff">
    <CODE>
      zsidekick.ZSideKickActions.innermost(view);
    </CODE>
  </ACTION>
  <ACTION NAME="zsidekick.interactiveRewriteCurrentWff">
    <CODE>
      zsidekick.ZSideKickActions.interactive_rewrite(view);
    </CODE>
  </ACTION>
  <ACTION NAME="zsidekick.disableTypechecking">
    <CODE>
      buffer.putBooleanProperty("zsidekick.disable-typechecking",
                ! buffer.getBooleanProperty("zsidekick.disable-typechecking"));
    </CODE>
    <IS_SELECTED>
      return buffer.getBooleanProperty("zsidekick.disable-typechecking");
    </IS_SELECTED>
  </ACTION>
  <ACTION NAME="zsidekick.show_current_sm_options">
    <CODE>
      zsidekick.ZSideKickActions.showOptions(view);
    </CODE>
  </ACTION>
  <ACTION NAME="zsidekick.reset_sm">
    <CODE>
      jEdit.setBooleanProperty("zsidekick.reset_sm", true);
    </CODE>
  </ACTION>
  <!--
  <ACTION NAME="zsidekick.showCompleteTree">
    <CODE>
      buffer.putBooleanProperty(ZSideKickPlugin.PROPERTY_PREFIX +
            ZSideKickPlugin.PROP_SHOW_COMPLETE_TREE,
                ! buffer.getBooleanProperty(ZSideKickPlugin.PROPERTY_PREFIX +
            ZSideKickPlugin.PROP_SHOW_COMPLETE_TREE));
    </CODE>
    <IS_SELECTED>
      return buffer.getBooleanProperty(ZSideKickPlugin.PROPERTY_PREFIX +
            ZSideKickPlugin.PROP_SHOW_COMPLETE_TREE);
    </IS_SELECTED>
  </ACTION>
  -->
</ACTIONS>
