      Suite 330, Boston, MA  02111-1307  USA
    </xs:documentation>
    <xs:appinfo>
      <jaxb:schemaBindings>
        <jaxb:package name="net.sourceforge.czt.oz.jaxb.gen">
          <jaxb:javadoc>
            &lt;!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 3.2 Final//EN"&gt;
            &lt;html&gt;
            &lt;body&gt;
            &lt;p&gt;Classes generated by Jaxb.&lt;/p&gt;
            &lt;/body&gt;
            &lt;/html&gt;
          </jaxb:javadoc>
        </jaxb:package>
      </jaxb:schemaBindings>
      <gnast:schemaBindings>
        <gnast:package name="net.sourceforge.czt.oz">
          <gnast:package id="ast" name="ast">
            <gnast:generate id="factory" class="OzFactory" template="CoreFactory.vm"/>
          </gnast:package>
          <gnast:package id="impl" name="impl">
            <gnast:generate id="factoryImpl" class="OzFactoryImpl" template="CoreFactoryImpl.vm"/>
          </gnast:package>
          <gnast:package id="visitor" name="visitor"/>
          <gnast:package id="jaxb" name="jaxb"/>
          <gnast:package id="dom" name="dom"/>
        </gnast:package>
      </gnast:schemaBindings>
    </xs:appinfo>
  </xs:annotation>

  <xs:import namespace="http://czt.sourceforge.net/zml" schemaLocation="Z.xsd"/>

  <!-- the Paragraph hierarchy -->
  <xs:element name="ClassPara" type="OZ:ClassPara" substitutionGroup="Z:Para"/>
  <!-- the OperationExpr hierarchy -->
  <xs:element name="OperationExpr" type="OZ:OperationExpr" abstract="true"/>
  <xs:element name="DistConjOpExpr" type="OZ:DistOpExpr" substitutionGroup="OZ:OperationExpr"/>
  <xs:element name="DistChoiceOpExpr" type="OZ:DistOpExpr" substitutionGroup="OZ:OperationExpr"/>
  <xs:element name="DistSeqOpExpr" type="OZ:DistOpExpr" substitutionGroup="OZ:OperationExpr"/>
  <xs:element name="BasicOpExpr" type="OZ:BasicOpExpr" substitutionGroup="OZ:OperationExpr"/>
  <xs:element name="OpPromotionExpr" type="OZ:OpPromotionExpr" substitutionGroup="OZ:OperationExpr"/>
  <xs:element name="RenameOpExpr" type="OZ:RenameOpExpr" substitutionGroup="OZ:OperationExpr"/>
  <xs:element name="HideOpExpr" type="OZ:HideOpExpr" substitutionGroup="OZ:OperationExpr"/>
  <xs:element name="ConjOpExpr" type="OZ:TwoOpExpr" substitutionGroup="OZ:OperationExpr"/>
  <xs:element name="ParallelOpExpr" type="OZ:TwoOpExpr" substitutionGroup="OZ:OperationExpr"/>
  <xs:element name="AssoParallelOpExpr" type="OZ:TwoOpExpr" substitutionGroup="OZ:OperationExpr"/>
  <xs:element name="ExChoiceOpExpr" type="OZ:TwoOpExpr" substitutionGroup="OZ:OperationExpr"/>
  <xs:element name="SeqOpExpr" type="OZ:TwoOpExpr" substitutionGroup="OZ:OperationExpr"/>
  <xs:element name="ScopeEnrichOpExpr" type="OZ:TwoOpExpr" substitutionGroup="OZ:OperationExpr"/>
  <xs:element name="ParenOpExpr" type="OZ:OperationExpr" substitutionGroup="OZ:OperationExpr">
    <xs:annotation>
      <xs:documentation>Parenthesized operation expression</xs:documentation>
    </xs:annotation>
  </xs:element>
  <!-- Miscellaneous -->
  <xs:element name="FormalParameters" type="OZ:FormalParameters"/>
  <xs:element name="StringListType" type="OZ:StringListType"/>
  <xs:element name="InheritedClass" type="OZ:InheritedClass"/>
  <xs:element name="ActualParameters" type="OZ:ActualParameters"/>
  <xs:element name="State" type="OZ:State"/>
  <xs:element name="RenameList" type="OZ:RenameList"/>
  <xs:element name="InitialState" type="OZ:InitialState"/>
  <xs:element name="LocalDef" type="OZ:LocalDef"/>
  <xs:element name="SecondaryAttributes" type="OZ:SecondaryAttributes"/>
  <xs:element name="Operation" type="OZ:Operation"/>
  <xs:element name="OperationBox" type="OZ:OperationBox"/>
  <xs:element name="MainOpExpr" type="OZ:MainOpExpr"/>

  <!-- Definitions of the complexTypes -->
  <xs:complexType name="ClassPara">
    <xs:annotation>
      <xs:documentation>New addition of Object-Z construct: class definition added by Li Yuanfang</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="Z:Para">
        <xs:sequence>
          <xs:element name="Name" type="Z:DeclName"/>
          <xs:element ref="OZ:FormalParameters" minOccurs="0"/>
          <xs:element name="VisibilityList" type="OZ:StringListType" minOccurs="0"/>
          <xs:element ref="OZ:InheritedClass" minOccurs="0" maxOccurs="unbounded"/>
          <xs:element ref="OZ:LocalDef" minOccurs="0"/>
          <xs:element ref="OZ:State" minOccurs="0"/>
          <xs:element ref="OZ:InitialState" minOccurs="0"/>
          <xs:element ref="OZ:Operation" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="FormalParameters">
    <xs:complexContent>
      <xs:extension base="Z:TermA">
        <xs:sequence>
          <xs:element ref="Z:RefName" maxOccurs="unbounded"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="StringListType">
    <xs:complexContent>
      <xs:extension base="Z:TermA">
        <xs:sequence>
          <xs:element ref="Z:Name" maxOccurs="unbounded"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="InheritedClass">
    <xs:complexContent>
      <xs:extension base="Z:TermA">
        <xs:sequence>
          <xs:element name="Name" type="Z:RefName"/>
          <xs:element ref="OZ:ActualParameters" minOccurs="0"/>
          <xs:element ref="OZ:RenameList" minOccurs="0"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="ActualParameters">
    <xs:complexContent>
      <xs:extension base="Z:TermA">
        <xs:sequence>
          <xs:element ref="Z:Expr" maxOccurs="unbounded"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="RenameList">
    <xs:complexContent>
      <xs:extension base="Z:TermA">
        <xs:sequence>
          <xs:element ref="Z:RenameExpr"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="LocalDef">
    <xs:complexContent>
      <xs:extension base="Z:TermA">
        <xs:sequence>
          <xs:element ref="Z:GivenPara" minOccurs="0" maxOccurs="unbounded"/>
          <xs:element ref="Z:AxPara" minOccurs="0" maxOccurs="unbounded"/>
          <xs:element ref="Z:FreePara" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="State">
    <xs:complexContent>
      <xs:extension base="Z:TermA">
        <xs:sequence>
          <xs:element ref="Z:Decl" maxOccurs="unbounded"/>
          <xs:element ref="OZ:SecondaryAttributes" minOccurs="0" maxOccurs="unbounded"/>
          <xs:element ref="Z:Pred" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="SecondaryAttributes">
    <xs:annotation>
      <xs:documentation>Secondary attribute</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="Z:Decl"/>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="InitialState">
    <xs:complexContent>
      <xs:extension base="Z:TermA">
        <xs:sequence>
          <xs:element ref="Z:Pred" maxOccurs="unbounded"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="Operation">
    <xs:complexContent>
      <xs:extension base="Z:TermA">
        <xs:sequence>
          <xs:element name="Name" type="Z:DeclName"/>
          <xs:choice>
            <xs:annotation>
              <xs:appinfo>
                <jaxb:property name="OperandBoxOrExpr"/>
              </xs:appinfo>
            </xs:annotation>
            <xs:element ref="OZ:OperationBox"/>
            <xs:element ref="OZ:OperationExpr"/>
          </xs:choice>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="OperationBox">
    <xs:complexContent>
      <xs:extension base="Z:TermA">
        <xs:sequence>
          <xs:element name="DeltaList" type="OZ:StringListType" minOccurs="0"/>
          <xs:element ref="Z:Decl" minOccurs="0" maxOccurs="unbounded"/>
          <xs:element ref="Z:Pred" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="OperationExpr">
    <xs:annotation>
      <xs:documentation>Super type for all operation expressions</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="Z:TermA"/>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="MainOpExpr">
    <xs:complexContent>
      <xs:extension base="OZ:OperationExpr">
        <xs:sequence>
          <xs:element ref="Z:SchText"/>
          <xs:element ref="OZ:OperationExpr"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="DistOpExpr">
    <xs:complexContent>
      <xs:extension base="OZ:OperationExpr">
        <xs:sequence>
          <xs:element ref="OZ:MainOpExpr"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="BasicOpExpr">
    <xs:complexContent>
      <xs:extension base="OZ:OperationExpr">
        <xs:sequence>
          <xs:element name="DeltaList" type="OZ:StringListType" minOccurs="0"/>
          <xs:element ref="Z:SchText" minOccurs="0"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="OpPromotionExpr">
    <xs:annotation>
      <xs:documentation>The class name is now made to be optional to conform to TCOZ syntax -- Li Yuanfang</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="OZ:OperationExpr">
        <xs:sequence>
          <xs:element ref="Z:Expr" minOccurs="0"/>
          <xs:element name="OpName" type="Z:RefName"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="RenameOpExpr">
    <xs:complexContent>
      <xs:extension base="OZ:OperationExpr">
        <xs:sequence>
          <xs:element ref="OZ:OperationExpr"/>
          <xs:element ref="OZ:RenameList" minOccurs="0"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="HideOpExpr">
    <xs:complexContent>
      <xs:extension base="OZ:OperationExpr">
        <xs:sequence>
          <xs:element ref="OZ:OperationExpr"/>
          <xs:element name="HideName" type="Z:RefName" maxOccurs="unbounded"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="TwoOpExpr">
    <xs:annotation>
      <xs:documentation>This complexType is only for convenience purposes. No element in an actual XML document should be of TwoOpExpr type.</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="OZ:OperationExpr">
        <xs:sequence>
          <xs:element ref="OZ:OperationExpr">
            <xs:annotation>
              <xs:appinfo>
                <jaxb:property name="LeftOperationExpr"/>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element ref="OZ:OperationExpr">
            <xs:annotation>
              <xs:appinfo>
                <jaxb:property name="RightOperationExpr"/>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
</xs:schema>
