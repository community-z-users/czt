<web-app>

  <context-param>
    <param-name>testcases-directory</param-name>
    <param-value>/home/ga11/development/zml/zml2html/testcases</param-value>
  </context-param>

  <servlet>
    <servlet-name>
      control
    </servlet-name>
    <servlet-class>
      net.sourceforge.czt.zml2html.testing.clients.webclient.ControlServlet
