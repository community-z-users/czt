<?xml version="1.0" encoding="UTF-8"?>
<feature
      id="net.sourceforge.czt.eclipse.feature"
      label="%featureName"
      version="1.6.0.qualifier"
      provider-name="%providerName"
      plugin="net.sourceforge.czt.eclipse.feature.branding">

   <description url="http://czt.sourceforge.net/eclipse">
      %description
   </description>

   <copyright url="%copyrightURL">
      %copyright
   </copyright>

   <license url="%licenseURL">
      %license
   </license>

   <url>
      <update label="%updateSiteName" url="http://czt.sourceforge.net/eclipse/updates/releases/"/>
   </url>

   <requires>
      <import plugin="org.eclipse.core.runtime"/>
      <import plugin="org.eclipse.ui.workbench.texteditor"/>
      <import plugin="org.eclipse.jface.text"/>
      <import plugin="org.eclipse.ui"/>
      <import plugin="org.eclipse.ui.editors"/>
      <import plugin="org.eclipse.core.resources"/>
      <import plugin="org.eclipse.ui.ide"/>
      <import plugin="org.eclipse.ui.console"/>
      <import plugin="org.eclipse.ui.views"/>
      <import plugin="org.eclipse.search"/>
      <import plugin="org.eclipse.ui.forms"/>
      <import plugin="org.eclipse.core.runtime" version="3.7.0" match="greaterOrEqual"/>
      <import feature="net.sourceforge.czt.core.feature" version="1.6.0.qualifier"/>
   </requires>

   <plugin
         id="net.sourceforge.czt.eclipse.ui"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="net.sourceforge.czt.eclipse.doc"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="net.sourceforge.czt.eclipse.vcg.ui"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="net.sourceforge.czt.eclipse.feature.branding"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="net.sourceforge.czt.eclipse.core"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

