<xs:import namespace="http://czt.sourceforge.net/zml" schemaLocation="Z.xsd"/>
<xs:element name="SubstList" type="P:SubstList">
<xs:annotation>
<xs:appinfo>
<jaxb:class name="SubstListElement"/>
</xs:appinfo>
</xs:annotation>
</xs:element>
<xs:element name="Substitute" type="P:Substitute">
<xs:annotation>
<xs:appinfo>
<jaxb:class name="SubstituteElement"/>
</xs:appinfo>
</xs:annotation>
</xs:element>
<xs:element name="JokerExpr" type="P:JokerExpr" substitutionGroup="Z:Expr">
<xs:annotation>
<xs:appinfo>
<jaxb:class name="JokerExprElement"/>
</xs:appinfo>
</xs:annotation>
</xs:element>
<xs:element name="JokerPred" type="P:JokerPred" substitutionGroup="Z:Pred">
<xs:annotation>
<xs:appinfo>
<jaxb:class name="JokerPredElement"/>
</xs:appinfo>
</xs:annotation>
</xs:element>
<xs:complexType name="SubstList">
<xs:sequence>
<xs:element ref="P:Substitute" minOccurs="0" maxOccurs="unbounded"/>
</xs:sequence>
</xs:complexType>
<xs:complexType name="Substitute">
<xs:sequence>
<xs:element ref="Z:Expr" minOccurs="2" maxOccurs="2"/>
<xs:element ref="Z:Pred" minOccurs="2" maxOccurs="2"/>
</xs:sequence>
</xs:complexType>
<xs:complexType name="JokerExpr">
<xs:complexContent>
<xs:extension base="Z:Expr">
<xs:attribute name="Name" type="xs:string" use="optional"/>
</xs:extension>
</xs:complexContent>
</xs:complexType>
<xs:complexType name="JokerPred">
<xs:complexContent>
<xs:extension base="Z:Pred">
<xs:attribute name="Name" type="xs:string" use="optional"/>
</xs:extension>
</xs:complexContent>
</xs:complexType>
