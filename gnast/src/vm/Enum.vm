package ${project.AstPackage};

/**
#if(${javadoc.get(${Name})}) * ${javadoc.get(${Name})}
#end
 *
#if($author) * @author ${author}
#end
 */
public class ${Name}
{
#foreach($value in ${Values})
  public final static ${Name} ${value} = new ${Name}("${value}");
#end
  private final String name;

  /**
   * Only this class can construct instances.
   */
  private ${Name}(String name) { this.name = name; }

  public String toString() { return name; }

  public final int hashCode() { return super.hashCode(); }

  public final boolean equals(java.lang.Object o) { return super.equals(o); }

  public static ${Name} fromString(java.lang.String value) {
#foreach($value in ${Values})
    if (value.equals("${value}")) return ${value};
#end
    throw new IllegalArgumentException();
  }
}
